{
  "app_name": "DEILE",
  "version": "5.0.0",
  "debug": false,
  "working_directory": ".",
  "config_directory": "config",
  "logs_directory": "logs",
  "cache_directory": "cache",
  "log_level": "DEBUG",
  "log_to_file": true,
  "log_file_max_size": 10485760,
  "log_file_backup_count": 5,
  "default_model_provider": "gemini",
  "default_model_name": "gemini-1.5-pro-latest",
  "max_context_tokens": 8000,
  "auto_discover_tools": true,
  "enabled_tool_categories": [
    "file",
    "execution",
    "search"
  ],
  "max_tool_execution_time": 30,
  "auto_discover_parsers": true,
  "parser_cache_enabled": true,
  "parser_cache_ttl": 300,
  "context_optimization_enabled": true,
  "rag_enabled": false,
  "semantic_search_enabled": false,
  "embedding_model": "text-embedding-ada-002",
  "max_concurrent_requests": 10,
  "request_timeout": 120,
  "enable_caching": true,
  "cache_ttl": 3600,
  "enable_file_safety_checks": true,
  "allowed_file_extensions": [
    ".py",
    ".js",
    ".ts",
    ".html",
    ".css",
    ".md",
    ".txt",
    ".json",
    ".yaml",
    ".yml"
  ],
  "blocked_directories": [
    ".git",
    "__pycache__",
    "node_modules",
    ".env"
  ],
  "max_file_size_bytes": 1048576,
  "allow_all_file_types": true,
  "file_encoding_detection": true,
  "environment": "development"
}